set termguicolors
syntax enable
set background=dark

" GRUVBOX

"let g:gruvbox_contrast_dark = 'hard'
"let g:gruvbox_sign_column = 'bg'
"let g:gruvbox_color_column = 'bg'
colorscheme gruvbox

" DRACULA
"colorscheme dracula

" AYU
"let ayucolor="dark"
"colorscheme ayu

" CODEDARK
"colorscheme codedark

highlight Normal ctermbg=none guibg=none
"highlight clear LineNr
"highlight clear SignColumn
